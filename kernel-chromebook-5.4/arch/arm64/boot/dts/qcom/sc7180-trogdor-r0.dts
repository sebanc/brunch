	};

	pinconf {
		pins = "gpio104";
	};
};

&trackpad_int_1v8_odl {
	pinmux {
		pins = "gpio58";
	};

	pinconf {
		pins = "gpio58";
	};
};

/* PINCTRL - board-specific pinctrl */

&pm6150l_gpio {
	gpio-line-names = "AP_SUSPEND_L", /* TODO: Remove if doesn't work */
			  "",
			  "",
			  "",
			  "",
			  "",
			  "",
			  "",
			  "",
			  "",
			  "",
			  "";
};

&qup_uart3_default {
	/* account for swapped Bluetooth TX/RX and RTS/CTS lines */

	pinconf-cts {
		pins = "gpio39";
	};

	pinconf-rts-tx {
		pins = "gpio38", "gpio41";
	};

	pinconf-rx {
		pins = "gpio40";
	};
};

